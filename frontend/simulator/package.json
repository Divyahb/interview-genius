{
  "name": "simulator",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "simulator",
    "targets": {
      "build": {
        "executor": "@nx/webpack:webpack",
        "outputs": [
          "{options.outputPath}"
        ],
        "defaultConfiguration": "production",
        "options": {
          "compiler": "babel",
          "outputPath": "frontend/simulator/dist",
          "index": "frontend/simulator/src/index.html",
          "baseHref": "/",
          "main": "frontend/simulator/src/main.ts",
          "tsConfig": "frontend/simulator/tsconfig.app.json",
          "assets": [
            "frontend/simulator/src/favicon.ico",
            "frontend/simulator/src/assets"
          ],
          "styles": [
            "frontend/simulator/src/styles.css"
          ],
          "scripts": [],
          "webpackConfig": "frontend/simulator/webpack.config.ts",
          "postcssConfig": "frontend/simulator/postcss.config.js"
        },
        "configurations": {
          "development": {
            "extractLicenses": false,
            "optimization": false,
            "sourceMap": true,
            "vendorChunk": true
          },
          "production": {
            "fileReplacements": [
              {
                "replace": "frontend/simulator/src/environments/environment.ts",
                "with": "frontend/simulator/src/environments/environment.prod.ts"
              }
            ],
            "optimization": true,
            "outputHashing": "all",
            "sourceMap": false,
            "namedChunks": false,
            "extractLicenses": true,
            "vendorChunk": false,
            "webpackConfig": "frontend/simulator/webpack.config.prod.ts"
          }
        },
        "dependsOn": [
          "^build",
          "typecheck"
        ]
      },
      "serve": {
        "executor": "@nx/react:module-federation-dev-server",
        "defaultConfiguration": "development",
        "options": {
          "buildTarget": "simulator:build",
          "hmr": true,
          "port": 4204
        },
        "configurations": {
          "development": {
            "buildTarget": "simulator:build:development"
          },
          "production": {
            "buildTarget": "simulator:build:production",
            "hmr": false
          }
        },
        "dependsOn": [
          "typecheck"
        ]
      },
      "serve-static": {
        "executor": "@nx/react:module-federation-static-server",
        "defaultConfiguration": "production",
        "options": {
          "serveTarget": "simulator:serve"
        },
        "configurations": {
          "development": {
            "serveTarget": "simulator:serve:development"
          },
          "production": {
            "serveTarget": "simulator:serve:production"
          }
        }
      }
    }
  },
  "exports": {
    "./Module": {
      "@interview-genius/source": "./src/remote-entry.ts",
      "types": "./src/remote-entry.ts",
      "import": "./src/remote-entry.ts",
      "default": "./src/remote-entry.ts"
    }
  },
  "types": "./src/remote-entry.ts"
}
